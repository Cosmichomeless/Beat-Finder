{
  "expo": {
    "name": "Beat-Finder-Frontend",
    "slug": "Beat-Finder-Frontend",
    "version": "0.1",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "scheme": "beatfinder",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cosmichomeless.BeatFinderFrontend"
    },
    "android": {
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cosmichomeless.BeatFinderFrontend",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "beatfinder"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-asset",
      "expo-web-browser"
    ]
  }
}
